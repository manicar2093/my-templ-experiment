package userpages

import "templ-demo/internal/domain/models"
import "github.com/manicar2093/gormpager"
import "templ-demo/cmd/web/ui/layouts"
import "templ-demo/cmd/web/ui/components/button"

templ UserIndex(page *gormpager.Page[models.User]) {
	@layouts.Initial() {
		@UsersTable(page)
		@button.Button(button.Props{
			Variant: button.VariantLink,
			Href: "/users/new",
		}) {
			Crear usuario
		}
	}
}
